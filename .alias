alias svncup="svn cleanup .; svn up --force"
alias svnd="svn diff -x -b > diff.txt; vi diff.txt +0;rm diff.txt"
alias svnh="head .svn/entries"
alias svns="svn status"
alias svnup="svn up"
alias tail="tail -f -n -20"
alias tcperror_log="tail /etc/httpd/logs/error_log"
