#!/bin/bash

# SCM displayer helper
#
# @author Michael Cannon <michael@typo3vagabond.com>

# set exports
echo
echo export CVS_RSH=${CVS_RSH}
echo export CVSROOT=${CVSROOT}
echo cvs co -d .skel root
echo

echo
echo svn co https://peimic.svn.cvsdude.com/skel/trunk .skel
echo

echo
echo svn co https://peimic.svn.cvsdude.com/skel/trunk/.ssh .
echo

echo
echo git clone git@github.com:typo3-vagabond/skel.git .skel
echo

echo
echo git clone https://github.com/typo3-vagabond/skel.git .skel
echo

echo
echo git clone git_peimic@peimic.git.cloudforge.com/skelcustom.git ~/bin/custom
echo

echo
echo mv .skel/ .skel.o
echo git clone https://github.com/typo3-vagabond/skel.git .skel
echo sh .skel/bin/shellinks
echo