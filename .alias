alias df="df -h"
alias diff="diff -bru -x .git -x .svn -x CVS"
alias du1="df; du > du.txt; vi du.txt; rm du.txt"
alias du="du --max-depth=1 -h | sort -r"
alias ferrors="find . \( -name 'errors' -o -name 'error_log' \) -type f -exec vim {} \;"
alias git2cl="git log > changelog.txt"
alias gita="git add"
alias gitau="git add -u"
alias gitc="git commit"
alias gitca="git commit -a"
