alias rmtrash="rm -rf ~/.Trash/* ~/tmp/* /Volumes/PEIMIC/.Trash /Volumes/*/.Trash"
alias rsync="rsync -Pahz -e ssh --stats"
alias sbdev="~/bin/fixwebsitepermissions stonebriar dev"
alias sblive="~/bin/fixwebsitepermissions stonebriar"
