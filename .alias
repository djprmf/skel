alias gith="grep url .git/config"
alias gitld="git log --oneline --graph --decorate"
alias gitpom="git push origin master"
alias gitpomg="git push origin HEAD:refs/for/master"
alias gitpull="git pull"
alias gitpush="git push"
alias gits="git status"
alias grep="grep -nrsI"
alias hisg="history | \grep"
alias i2csvnfix="find . -type f -name entries -exec perl -pi -e 's#109.239.60.22#svn.in2code.de#g' {} \;"
alias kcvsdir="find . -name CVS -type d -exec rm -rf {} \;"
alias kdiff="find . -name 'diff.txt' -type f -exec rm {} \;"
