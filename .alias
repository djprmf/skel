alias ..2="cd ../.."
alias ..3="cd ../../.."
alias ..4="cd ../../../.."
alias ..5="cd ../../../../.."
alias ..="cd .."
alias c2c="cvs2cl.pl --branches --gmt --hide-filenames --no-indent --prune --revisions --tagdates --tags; rm CHANGELOG.bak; cvs ci -m ''"
alias catmysql="cat ~/.ssh/l2l_config/mysql"
alias ccd="cls; cd"
alias cd..="cd .."
alias cls="clear"
alias cpdev="~/bin/fixwebsitepermissions cpanel dev"
alias cphp="find . -name '*.php' -exec php -l {} \;"
