#!/bin/sh

# @author Michael Cannon <michael@peimic.com>
# @version $Id: siteopener,v 1.1.1.1 2010/04/14 09:05:44 peimic.comprock Exp $

# have an arguement
if [ ! "$1" ]
then
	echo "Usage: `basename $0` [file]"
	exit 65
fi

FILE=$1

for DOMAIN in `cat ${FILE}`
do
	echo open http://${DOMAIN}
	open http://${DOMAIN}
done