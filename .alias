alias ksvndir="find . -name '.svn' -type d -exec rm -rf {} \;"
alias la="ls -l -a"
alias lh="ls -A --ignore='[^.]*'"
alias ll="ls -l"
alias llr="ll -R"
alias lr="ls -R --ignore='CVS' --ignore='.svn' --ignore='.git'"
alias lw="watch 'ls -alh'"
alias mkpasswd12="mkpasswd -l 12 -2 -s 0"
alias mkpasswd="mkpasswd -l 10 -2 -s 2"
alias ping="ping -c 5 "
alias portup="sudo port selfupdate; sudo port upgrade outdated"
